<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <threadsafe>false</threadsafe>
  <sessions-enabled>true</sessions-enabled>
  <runtime>java8</runtime>
  <version>1.0</version>

  <static-files>
    <include path="/**.html"></include>
    <include path="/**.css"></include>
    <include path="/**.js"></include>
    <include path="/**.java"></include>
    <include path="/favicon.ico"></include>
  </static-files>
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
  </system-properties>
  <!-- [START env_variables] -->
  <env-variables>
    <env-var name="ENDPOINTS_SERVICE_NAME" value="projet-tinygram-tf.appspot.com" />
    <env-var name="DEFAULT_BUCKET_NAME" value="projet-tinygram-tf.appspot.com" />

  </env-variables>

  <!-- [END env_variables] -->
</appengine-web-app>